section(class='section our-services-section')
  .container
    .section-title Наши услуги

    .services-switcher
      ul.ss-list
        li.active: a.ss-auto: span Автоперевозки
        li: a.ss-freeze: span Перевозки рефрежиратором
        li: a.ss-train: span Ж/Д перевозки
        li: a.ss-sea: span Морские перевозки
        li: a.ss-air: span Авиаперевозки

      .ss-content
        .active
          img(src=imgPath + 'ss-freeze.png')
          .ss-info
            .ss-title Автоперевозки
